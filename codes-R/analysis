   "source": [
    "def count_victories(df):\n",
    "    colnames = [\"-2l\", \"AIC\", \"CAIC\", \"BIC\", \"HQIC\"]\n",
    "    rownames = df[\"model\"].unique()\n",
    "    analysis = pd.DataFrame(data=np.zeros([len(rownames), 5]), columns=colnames)\n",
    "    analysis.index = rownames\n",
    "\n",
    "    #tempDf = df[[\"identifier\", \"-2l\", \"AIC\", \"BIC\", \"HQIC\"]]\n",
    "    #groups = tempDf.groupby(\"identifier\")\n",
    "    #print(groups.idxmin() % len(rownames))\n",
    "    #print(df)\n",
    "    #asdsdas\n",
    "\n",
    "    for u in df[\"identifier\"].unique():\n",
    "        slice = df[df[\"identifier\"] == u]\n",
    "        for metric in colnames:\n",
    "            bestModel = slice[\"model\"][slice[metric].idxmin()]\n",
    "            # print(bestModel)\n",
    "            analysis.loc[bestModel, metric] += 1\n",
    "            # print(analysis.loc[m, metric])\n",
    "\n",
    "    print(analysis)\n",
    "\n",
    "count_victories(df)\n",
    "\n",
    "for directory in df[\"directory\"].unique():\n",
    "    print()\n",
    "    print(directory)\n",
    "    slice = df[df[\"directory\"] == directory]\n",
    "    count_victories(slice)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## The KW-CWG model wins in most cases, but not all\n",
    "\n",
    "## Lets see whether estimating C is better or not"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/html": [
       "<div>\n",
       "<style scoped>\n",
       "    .dataframe tbody tr th:only-of-type {\n",
       "        vertical-align: middle;\n",
       "    }\n",
       "\n",
       "    .dataframe tbody tr th {\n",
       "        vertical-align: top;\n",
       "    }\n",
       "\n",
       "    .dataframe thead th {\n",
       "        text-align: right;\n",
       "    }\n",
       "</style>\n",
       "<table border=\"1\" class=\"dataframe\">\n",
       "  <thead>\n",
       "    <tr style=\"text-align: right;\">\n",
       "      <th></th>\n",
       "      <th>directory</th>\n",
       "      <th>-2l</th>\n",
       "      <th>AIC</th>\n",
